me:'PostList',\n    components:{\n      SpinnerComponent,\n      HeaderComponent\n    },\n    data(){\n      return{\n        headingValue : \"Post Listing Page\",\n        loading:false,\n        errorMessage:null,\n        postList:[]\n      }\n    },\n    mounted(){\n      this.isLoading = true;\n      this.postListAction().then((response) => {\n        if (response.status === 200) {\n          this.postList = response.data.results;\n          this.isLoading = false;\n        }\n      });\n    },\n    methods:{\n      ...mapActions({\n        postListAction: 'postList',\n      }),\n  \n      },\n      computed: {\n        ...mapGetters({\n          postList: 'getPostList',\n        })\n      }\n    \n}\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}