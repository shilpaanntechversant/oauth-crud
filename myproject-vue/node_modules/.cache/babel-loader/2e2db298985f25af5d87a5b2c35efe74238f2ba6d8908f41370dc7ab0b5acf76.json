\n      SpinnerComponent,\n      HeaderComponent\n    },\n    data(){\n      return{\n        headingValue : \"Post Listing Page\",\n        loading:false,\n        errorMessage:null,\n        postList:[],\n        successMessage:'',\n        message:'',\n      }\n    },\n    mounted(){\n      if (localStorage.getItem('reloaded')) {\n        \n        localStorage.removeItem('reloaded');\n    } else {\n        localStorage.setItem('reloaded', '1');\n        location.reload();\n    }\n      this.isLoading = true;\n      this.postListAction().then((response) => {\n        if (response.status === 200) {\n          this.postList = response.data.results;\n          $('#post-table').DataTable( {\n                data: response.data.results,\n                columns: [\n                    { data: 'id' },\n                    { data: 'title' },\n                    {\n                      data: 'id',\n                      render: function(data){\n                        return `<div class=\"d-flex flex-column justify-content-center align-items-center\">\\\n                                  <button data-id=${data} class=\"view btn btn-warning my-1\">\\\n                                    <i class=\"fa fa-eye\"></i></button>\\\n                                  <button data-id=${data} class=\"edit btn btn-primary my-1\">\\\n                                    <i class=\"fa fa-pen\"></i></button>\n                                  <span class=\"delete\">\n                                    <button @click=\"postDelete(${data})\" data-id=${data} class=\"child btn btn-danger my-1\" ><i class=\"fa fa-trash\"></i></button> \\\n                                  </span>\n                                </div> `;\n                      }\n                    \n                    }\n                ]\n            } )\n          this.isLoading = false;\n        }\n      });\n    \n  \n\n      $(window).on('pageshow',function(){ \n        $(\".view\").click(function() {\n          let id = $(this).data('id');\n          window.location = '/post/details/' + id;\n        });\n        $(\".edit\").click(function() {\n          let id = $(this).data('id');\n          window.location = '/post/update/' + id;\n        });\n        $(\".delete\").click(function(event) {\n          //let id = $(this).data('id');\n\n          console.log(event)\n          console.log($(event.target).children('button.child').trigger( \"click\" ))\n          // $(this).trigger( \"click\" );\n          //  this.postDelete(id);\n        });\n        \n      }); \n    },\n    methods:{\n      ...mapActions({\n        postListAction: 'postList',\n        postDeleteAction: 'postDelete',\n      }),\n        postDelete(postId){\n          try{\n            this.postDeleteAction(postId).then((response) => {\n              if (response.data.status.code === 200){\n                \n                window.scrollTo(0, 0);\n                this.successMessage =response.data.status.message\n                setTimeout(() => {\n                  this.successMessage = '';\n                }, 3000);\n                setTimeout( () => this.$router.push({ path: '/post/list'}), 3000);\n              }else{\n                this.message ='Some Error Occured. Can you please try again'\n                setTimeout(() => {\n                  this.message = '';\n                }, 3000);\n\n              }\n            });\n          }\n        catch(error){\n          console.log(error.response.data.results)\n          this.errorMessage=error.response.data.results\n        }\n        },\n        \n      },\n      computed: {\n        ...mapGetters({\n          postList: 'getPostList',\n          successMessage: 'getPostSuccess',\n          errorMessage: 'getPostError',\n        })\n      }\n    \n}\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}