{"ast":null,"code":"import { mapActions, mapGetters } from 'vuex';\nimport SpinnerComponent from '@/components/SpinnerComponent.vue';\nimport HeaderComponent from '@/components/HeaderComponent.vue';\nexport default {\n  name: 'UserList',\n  components: {\n    SpinnerComponent,\n    HeaderComponent\n  },\n  data() {\n    return {\n      headingValue: \"User Listing Page\",\n      loading: false,\n      errorMessage: null,\n      usersList: [],\n      images: [\"https://64.media.tumblr.com/6eab4778e612f7f98a18cdafe1be7240/bea246efa674acbe-2f/s1280x1920/78c6b98e1b6f7f8e5ff19df626f065f1a039aa8b.jpg\", \"https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSuiuiYY--ENGKHCxoshOIavDEMIyHjKkpARw&usqp=CAU\", \"https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRuwj_u_pycUAyBRrOtiBG8CDSVdmUsbSs-1Q&usqp=CAU\", \"https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTrZS2DZqyMuxmhZ-HZ8gX7kMGL0qJMKnQhXQ&usqp=CAU\", \"https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQJbc1UnuHKsFpP95R0vC66W8poTAftQ2osQA&usqp=CAU\", \"https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQMddtEq_SZX6s95ScL4h43KrpdVh4ld8H9oQ&usqp=CAU\", \"https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTzhX25DiiLcOJu3_SR2L38hTT-Qljo2TYlwg&usqp=CAU\"],\n      timer: null,\n      currentIndex: 0\n    };\n  },\n  mounted() {\n    this.isLoading = true;\n    this.userListAction().then(response => {\n      console.log('dsd', response.status);\n      if (response.status === 200) {\n        this.usersList = response.data.results;\n        this.isLoading = false;\n      }\n    });\n    this.startSlide();\n  },\n  methods: {\n    ...mapActions({\n      userListAction: 'userList'\n    }),\n    startSlide: function () {\n      this.timer = setInterval(this.next, 4000);\n    },\n    next: function () {\n      this.currentIndex += 1;\n    },\n    prev: function () {\n      this.currentIndex -= 1;\n    }\n  },\n  computed: {\n    ...mapGetters({\n      userList: 'getUserList'\n    }),\n    currentImg: function () {\n      return this.images[Math.abs(this.currentIndex) % this.images.length];\n    }\n  }\n};","map":{"version":3,"mappings":"AA2DA,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAC7C,OAAOC,gBAAe,MAAQ,mCAAkC;AAChE,OAAOC,eAAc,MAAQ,kCAAiC;AAE9D,eAAe;EACXC,IAAI,EAAC,UAAU;EACfC,UAAU,EAAC;IACTH,gBAAgB;IAChBC;EACF,CAAC;EACDG,IAAI,GAAE;IACJ,OAAM;MACJC,YAAW,EAAI,mBAAmB;MAClCC,OAAO,EAAC,KAAK;MACbC,YAAY,EAAC,IAAI;MACjBC,SAAS,EAAC,EAAE;MACZC,MAAM,EAAE,CACR,0IAA0I,EAC1I,qGAAqG,EACrG,qGAAqG,EACrG,qGAAqG,EACrG,qGAAqG,EACrG,qGAAqG,EACrG,qGAAqG,CACtG;MACDC,KAAK,EAAE,IAAI;MACXC,YAAY,EAAE;IACd;EACF,CAAC;EACDC,OAAO,GAAE;IACP,IAAI,CAACC,SAAQ,GAAI,IAAI;IACrB,IAAI,CAACC,cAAc,EAAE,CAACC,IAAI,CAAEC,QAAQ,IAAK;MACtCC,OAAO,CAACC,GAAG,CAAC,KAAK,EAAEF,QAAQ,CAACG,MAAM;MACnC,IAAIH,QAAQ,CAACG,MAAK,KAAM,GAAG,EAAE;QAC3B,IAAI,CAACX,SAAQ,GAAIQ,QAAQ,CAACZ,IAAI,CAACgB,OAAO;QACtC,IAAI,CAACP,SAAQ,GAAI,KAAK;MACxB;IACF,CAAC,CAAC;IACF,IAAI,CAACQ,UAAU,EAAE;EACnB,CAAC;EACDC,OAAO,EAAC;IACN,GAAGxB,UAAU,CAAC;MACZgB,cAAc,EAAE;IAClB,CAAC,CAAC;IACFO,UAAU,EAAE,YAAW;MACrB,IAAI,CAACX,KAAI,GAAIa,WAAW,CAAC,IAAI,CAACC,IAAI,EAAE,IAAI,CAAC;IAC3C,CAAC;IACDA,IAAI,EAAE,YAAW;MACf,IAAI,CAACb,YAAW,IAAK,CAAC;IACxB,CAAC;IACDc,IAAI,EAAE,YAAW;MACf,IAAI,CAACd,YAAW,IAAK,CAAC;IACxB;EACF,CAAC;EACDe,QAAQ,EAAE;IACR,GAAG3B,UAAU,CAAC;MACZ4B,QAAQ,EAAE;IACZ,CAAC,CAAC;IACFC,UAAU,EAAE,YAAW;MACrB,OAAO,IAAI,CAACnB,MAAM,CAACoB,IAAI,CAACC,GAAG,CAAC,IAAI,CAACnB,YAAY,IAAI,IAAI,CAACF,MAAM,CAACsB,MAAM,CAAC;IACtE;EACF;AAEJ","names":["mapActions","mapGetters","SpinnerComponent","HeaderComponent","name","components","data","headingValue","loading","errorMessage","usersList","images","timer","currentIndex","mounted","isLoading","userListAction","then","response","console","log","status","results","startSlide","methods","setInterval","next","prev","computed","userList","currentImg","Math","abs","length"],"sourceRoot":"","sources":["/home/shilpa/Public/my-work/code/demo-authentication/authenticate-vue/src/views/UserList.vue"],"sourcesContent":["<template>\n <HeaderComponent :heading=\"headingValue\" />\n  <template>\n    <div>\n      <transition-group name=\"fade\" tag=\"div\">\n        <!-- <div v-for=\"i in [currentIndex]\" :key=\"i\">\n          <img class=\"slider-img\" :src=\"currentImg\" />\n        </div> -->\n      </transition-group>\n      <a class=\"prev\" @click=\"prev\" href=\"#\">&#10094; Previous</a>\n      <a class=\"next\" @click=\"next\" href=\"#\">&#10095; Next</a>\n    </div>\n  </template>\n <!--spinner -->\n <div v-if=\"loading === true\">\n  <div class=\"container\">\n    <div class=\"row\">\n      <div class=\"col\">\n        <SpinnerComponent/>\n      </div>\n    </div>\n  </div>\n\n </div>\n <div class=\"container mt-3\" >\n  <div class=\"row\">\n   <div class=\"col-md-6\" v-for=\"user of usersList\" :key=\"user.id\"> \n      <div class=\"card my-2 list-group-item-success shadow-lg\" >\n        <div class=\"card-body\">\n          <div class=\"row align-items-center\">\n            <div class=\"col-sm-4\">\n                <img class=\"user-img\" src=\"https://icons.veryicon.com/png/o/internet--web/prejudice/user-128.png\">\n            </div>\n            <div class=\"col-sm-7\">\n              <ul class=\"list-group\">\n                <li class=\"list-group-item\">Username: <span class=\"fw-bold\">{{user.username}} </span></li>\n                <li class=\"list-group-item\">Email: <span class=\"fw-bold\">{{user.email}}</span></li>\n              </ul>\n\n            </div>\n            <!-- <div class=\"col-sm-1 d-flex flex-column justify-content-center align-items-center\">\n              <router-link to=\"\" class=\"btn btn-warning my-1\">\n                <i class=\"fa fa-eye\"></i>\n              </router-link>\n              <router-link to=\"\" class=\"btn btn-primary my-1\">\n                <i class=\"fa fa-pen\"></i>\n              </router-link>\n              <button class=\"btn btn-danger my-1\" ><i class=\"fa fa-trash\"></i></button>\n            </div> -->\n          </div>\n        </div>\n      </div>\n   </div>\n  </div>\n </div>\n  \n</template>\n\n<script>\nimport { mapActions, mapGetters } from 'vuex';\nimport SpinnerComponent from  '@/components/SpinnerComponent.vue'\nimport HeaderComponent from  '@/components/HeaderComponent.vue'\n\nexport default {\n    name:'UserList',\n    components:{\n      SpinnerComponent,\n      HeaderComponent\n    },\n    data(){\n      return{\n        headingValue : \"User Listing Page\",\n        loading:false,\n        errorMessage:null,\n        usersList:[],\n        images: [\n        \"https://64.media.tumblr.com/6eab4778e612f7f98a18cdafe1be7240/bea246efa674acbe-2f/s1280x1920/78c6b98e1b6f7f8e5ff19df626f065f1a039aa8b.jpg\",\n        \"https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSuiuiYY--ENGKHCxoshOIavDEMIyHjKkpARw&usqp=CAU\",\n        \"https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRuwj_u_pycUAyBRrOtiBG8CDSVdmUsbSs-1Q&usqp=CAU\",\n        \"https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTrZS2DZqyMuxmhZ-HZ8gX7kMGL0qJMKnQhXQ&usqp=CAU\",\n        \"https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQJbc1UnuHKsFpP95R0vC66W8poTAftQ2osQA&usqp=CAU\",\n        \"https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQMddtEq_SZX6s95ScL4h43KrpdVh4ld8H9oQ&usqp=CAU\",\n        \"https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTzhX25DiiLcOJu3_SR2L38hTT-Qljo2TYlwg&usqp=CAU\",\n      ],\n      timer: null,\n      currentIndex: 0\n      }\n    },\n    mounted(){\n      this.isLoading = true;\n      this.userListAction().then((response) => {\n         console.log('dsd', response.status)\n        if (response.status === 200) {\n          this.usersList = response.data.results;\n          this.isLoading = false;\n        }\n      });\n      this.startSlide();\n    },\n    methods:{\n      ...mapActions({\n        userListAction: 'userList',\n      }),\n      startSlide: function() {\n        this.timer = setInterval(this.next, 4000);\n      },\n      next: function() {\n        this.currentIndex += 1;\n      },\n      prev: function() {\n        this.currentIndex -= 1;\n      },\n    },\n    computed: {\n      ...mapGetters({\n        userList: 'getUserList',\n      }),\n      currentImg: function() {\n        return this.images[Math.abs(this.currentIndex) % this.images.length];\n      }\n    }\n    \n}\n</script>\n\n<style>\n.fade-enter-active,\n.fade-leave-active {\n  transition: all 0.9s ease;\n  overflow: hidden;\n  visibility: visible;\n  position: absolute;\n  width:100%;\n  opacity: 1;\n}\n\n.fade-enter,\n.fade-leave-to {\n  visibility: hidden;\n  width:100%;\n  opacity: 0;\n}\n\n.slider-img {\n  height:600px;\n  width:100%\n}\n\n.prev, .next {\n  cursor: pointer;\n  position: absolute;\n  top: 40%;\n  width: auto;\n  padding: 16px;\n  color: white;\n  font-weight: bold;\n  font-size: 18px;\n  transition: 0.7s ease;\n  border-radius: 0 4px 4px 0;\n  text-decoration: none;\n  user-select: none;\n}\n\n.next {\n  right: 0;\n}\n\n.prev {\n  left: 0;\n}\n\n.prev:hover, .next:hover {\n  background-color: rgba(0,0,0,0.9);\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}