{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions, mapGetters } from 'vuex';\nexport default {\n  name: 'RegistrationView',\n  data() {\n    return {\n      userName: '',\n      email: '',\n      password: '',\n      confirmPassword: '',\n      errorMessage: {},\n      successMessage: '',\n      message: ''\n    };\n  },\n  methods: {\n    ...mapActions({\n      userSignupAction: 'userSignup'\n    }),\n    registerUser() {\n      console.log(this.signupErrors.results, 'ds');\n      this.signupErrors.results = '';\n      if (this.password != this.confirmPassword) {\n        this.signupErrors.resultsconfirm_password = ['Confirm Password is not same as Password'];\n      } else {\n        try {\n          const userData = {\n            username: this.userName,\n            email: this.email,\n            password: this.password,\n            confirm_password: this.confirmPassword\n          };\n          this.userSignupAction(userData).then(response => {\n            this.userName = '';\n            this.email = '';\n            this.password = '';\n            this.confirmPassword = '';\n            if (response.data.status.code === 200) {\n              window.scrollTo(0, 0);\n              this.successMessage = response.data.status.message;\n              setTimeout(() => {\n                this.successMessage = '';\n              }, 3000);\n              setTimeout(() => this.$router.push({\n                path: '/'\n              }), 5000);\n            } else {\n              this.message = 'Some Error Occured. Can you please try again';\n              setTimeout(() => {\n                this.message = '';\n              }, 4000);\n              setTimeout(() => this.$router.push({\n                path: '/register'\n              }), 5000);\n            }\n          });\n        } catch (error) {\n          console.log(error);\n          this.message = error.response.data.results;\n        }\n      }\n    }\n  },\n  computed: {\n    ...mapGetters({\n      signupErrors: 'getSignupErrors',\n      signupSuccess: 'getSignupSuccess'\n    })\n  }\n};","map":{"version":3,"mappings":";AA6DA,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAE7C,eAAe;EACXC,IAAI,EAAC,kBAAkB;EACvBC,IAAI,GAAE;IACF,OAAM;MACFC,QAAQ,EAAC,EAAE;MACXC,KAAK,EAAC,EAAE;MACRC,QAAQ,EAAC,EAAE;MACXC,eAAe,EAAC,EAAE;MAClBC,YAAY,EAAC,CAAC,CAAC;MACfC,cAAc,EAAC,EAAE;MACjBC,OAAO,EAAC;IACZ;EACJ,CAAC;EACDC,OAAO,EAAC;IACN,GAAGX,UAAU,CAAC;MACZY,gBAAgB,EAAE;IACpB,CAAC,CAAC;IACFC,YAAY,GAAE;MACZC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACC,YAAY,CAACC,OAAO,EAAC,IAAI;MAC1C,IAAI,CAACD,YAAY,CAACC,OAAM,GAAI,EAAC;MAC7B,IAAG,IAAI,CAACX,QAAO,IAAK,IAAI,CAACC,eAAe,EAAC;QACvC,IAAI,CAACS,YAAY,CAACE,uBAAsB,GAAG,CAAC,0CAA0C;MACxF,CAAC,MAAI;QACH,IAAI;UACF,MAAMC,QAAO,GAAI;YACfC,QAAQ,EAAE,IAAI,CAAChB,QAAQ;YACvBC,KAAK,EAAE,IAAI,CAACA,KAAK;YACjBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;YACvBe,gBAAgB,EAAE,IAAI,CAACd;UACzB,CAAC;UACD,IAAI,CAACK,gBAAgB,CAACO,QAAQ,CAAC,CAACG,IAAI,CAAEC,QAAQ,IAAK;YACjD,IAAI,CAACnB,QAAO,GAAI,EAAE;YAClB,IAAI,CAACC,KAAI,GAAI,EAAE;YACf,IAAI,CAACC,QAAO,GAAI,EAAE;YAClB,IAAI,CAACC,eAAc,GAAI,EAAE;YACzB,IAAIgB,QAAQ,CAACpB,IAAI,CAACqB,MAAM,CAACC,IAAG,KAAM,GAAG,EAAE;cACrCC,MAAM,CAACC,QAAQ,CAAC,CAAC,EAAE,CAAC,CAAC;cACrB,IAAI,CAAClB,cAAa,GAAIc,QAAQ,CAACpB,IAAI,CAACqB,MAAM,CAACd,OAAO;cAClDkB,UAAU,CAAC,MAAM;gBACf,IAAI,CAACnB,cAAa,GAAI,EAAE;cAC1B,CAAC,EAAE,IAAI,CAAC;cACRmB,UAAU,CAAE,MAAM,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;gBAAEC,IAAI,EAAE;cAAG,CAAC,CAAC,EAAE,IAAI,CAAC;YAE1D,OAAO;cACL,IAAI,CAACrB,OAAM,GAAG,8CAA6C;cAC3DkB,UAAU,CAAC,MAAM;gBACX,IAAI,CAAClB,OAAM,GAAI,EAAE;cACnB,CAAC,EAAE,IAAI,CAAC;cACZkB,UAAU,CAAE,MAAM,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;gBAAEC,IAAI,EAAE;cAAW,CAAC,CAAC,EAAE,IAAI,CAAC;YAClE;UACF,CAAC,CAAC;QACJ,EAAE,OAAOC,KAAK,EAAE;UACdlB,OAAO,CAACC,GAAG,CAACiB,KAAK;UACjB,IAAI,CAACtB,OAAO,GAACsB,KAAK,CAACT,QAAQ,CAACpB,IAAI,CAACc,OAAM;QACzC;MAGF;IAGF;EACF,CAAC;EACDgB,QAAQ,EAAE;IACV,GAAGhC,UAAU,CAAC;MACVe,YAAY,EAAE,iBAAiB;MAC/BkB,aAAa,EAAE;IACjB,CAAC;EACH;AACJ","names":["mapActions","map