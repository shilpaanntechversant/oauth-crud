;\n      this.userListAction(this.pageParam).then(response => {\n        console.log('dsd', response.status);\n        if (response.status === 200) {\n          this.usersList = response.data.results;\n          this.nextValue = response.data.next;\n          this.previousValue = response.data.previous;\n          this.isLoading = false;\n        }\n      });\n    },\n    nextPage() {\n      this.pageParam = this.nextValue.split(\"page=\")[1];\n      this.isLoading = true;\n      this.userListAction(this.pageParam).then(response => {\n        console.log('dsd', response.status);\n        if (response.status === 200) {\n          this.usersList = response.data.results;\n          this.nextValue = response.data.next;\n          this.previousValue = response.data.previous;\n          this.isLoading = false;\n        }\n      });\n    },\n    next: function () {\n      this.currentIndex += 1;\n    },\n    prev: function () {\n      this.currentIndex -= 1;\n    }\n  },\n  computed: {\n    ...mapGetters({\n      userList: 'getUserList'\n    }),\n    currentImg: function () {\n      return this.images[Math.abs(this.currentIndex) % this.images.length];\n    }\n  }\n};","map":{"version":3,"mappings":"AA8GA,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAE7C,OAAOC,gBAAe,MAAQ,mCAAkC;AAChE,OAAOC,eAAc,MAAQ,kCAAiC;AAE9D,eAAe;EACXC,IAAI,EAAC,UAAU;EACfC,UAAU,EAAC;IACTH,gBAAgB;IAChBC;EAEF,CAAC;EACDG,IAAI,GAAE;IACJ,OAAM;MACJC,SAAS,EAAE,GAAG;MACdC,YAAW,EAAI,mBAAmB;MAClCC,OAAO,EAAC,KAAK;MACbC,YAAY,EAAC,IAAI;MACjBC,SAAS,EAAC,EAAE;MACZC,MAAM,EAAE,CACN,yJAAyJ,EACzJ,6CAA6C,EAC7C,2JAA2J,EAC3J,oKAAoK,EACpK,yEAAwE,CAC1E;MACFC,KAAK,EAAE,IAAI;MACXC,YAAY,EAAE,CAAC;MACfC,SAAS,EAAC,IAAI;MACdC,aAAa,EAAC,IAAI;MAClBC,SAAS,EAAC;IACV;EACF,CAAC;EACDC,OAAO,GAAE;IACP,IAAI,CAACC,SAAQ,GAAI,IAAI;IACrB,IAAI,CAACC,cAAc,CAAC,IAAI,CAACH,SAAS,CAAC,CAACI,IAAI,CAAEC,QAAQ,IAAK;MACpDC,OAAO,CAACC,GAAG,CAAC,KAAK,EAAEF,QAAQ,CAACG,MAAM;MACnC,IAAIH,QAAQ,CAACG,MAAK,KAAM,GAAG,EAAE;QAC3B,IAAI,CAACd,SAAQ,GAAIW,QAAQ,CAAChB,IAAI,CAACoB,OAAO;