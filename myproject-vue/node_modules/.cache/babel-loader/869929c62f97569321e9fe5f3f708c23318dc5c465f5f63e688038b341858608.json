extValue = response.data.next;\n          this.previousValue = response.data.previous;\n          this.isLoading = false;\n        }\n      });\n    },\n    next: function () {\n      this.currentIndex += 1;\n    },\n    prev: function () {\n      this.currentIndex -= 1;\n    }\n  },\n  computed: {\n    ...mapGetters({\n      userList: 'getUserList'\n    }),\n    currentImg: function () {\n      return this.images[Math.abs(this.currentIndex) % this.images.length];\n    }\n  }\n};","map":{"version":3,"mappings":"AAwDA,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAE7C,OAAOC,gBAAe,MAAQ,mCAAkC;AAChE,OAAOC,eAAc,MAAQ,kCAAiC;AAE9D,eAAe;EACXC,IAAI,EAAC,UAAU;EACfC,UAAU,EAAC;IACTH,gBAAgB;IAChBC;EACF,CAAC;EACDG,IAAI,GAAE;IACJ,OAAM;MACJC,YAAW,EAAI,mBAAmB;MAClCC,OAAO,EAAC,KAAK;MACbC,YAAY,EAAC,IAAI;MACjBC,SAAS,EAAC,EAAE;MACZC,SAAS,EAAC,IAAI;MACdC,aAAa,EAAC,IAAI;MAClBC,SAAS,EAAC;IACZ;EACF,CAAC;EACDC,OAAO,GAAE;IACP,IAAI,CAACC,SAAQ,GAAI,IAAI;IACrB,IAAI,CAACC,cAAc,CAAC,IAAI,CAACH,SAAS,CAAC,CAACI,IAAI,CAAEC,QAAQ,IAAK;MACpDC,OAAO,CAACC,GAAG,CAAC,KAAK,EAAEF,QAAQ,CAACG,MAAM;MACnC,IAAIH,QAAQ,CAACG,MAAK,KAAM,GAAG,EAAE;QAC3B,IAAI,CAACX,SAAQ,GAAIQ,QAAQ,CAACZ,IAAI,CAACgB,OAAO;QACtC,IAAI,CAACX,SAAS,GAAEO,QAAQ,CAACZ,IAAI,CAACiB,IAAI;QAClC,IAAI,CAACX,aAAa,GAAEM,QAAQ,CAACZ,IAAI,CAACkB,QAAQ;QAC1C,IAAI,CAACT,SAAQ,GAAI,KAAK;MACxB;IACF,CAAC,CAAC;IACH,IAAI,CAACU,UAAU,EAAE;EAClB,CAAC;EACDC,OAAO,EAAC;IACN,GAAG1B,UAAU,CAAC;MACZgB,cAAc,EAAE;IAClB,CAAC,CAAC;IACFS,UAAU,EAAE,YAAW;MACrB,IAAI,CAACE,KAAI,GAAIC,WAAW,CAAC,IAAI,CAACL,IAAI,EAAE,IAAI,CAAC;IAC3C,CAAC;IAEDM,QAAQ,GAAE;MACN,IAAI,CAAChB,SAAS,GAAC,IAAI,CAACD,aAAa,CAACkB,KAAK,CAAC,OAAO,CAAC,CAAC,CAAC;MAClD,IAAG,IAAI,CAACjB,SAAQ,KAAMkB,SAAS,EAAC;QAC9B,IAAI,CAAClB,SAAQ,GAAG;MAClB;MAEA,IAAI,CAACE,SAAQ,GAAI,IAAI;MACrB,IAAI,CAACC,cAAc,CAAC,IAAI,CAACH,SAAS,CAAC,CAACI,IAAI,CAAEC,QAAQ,IAAK;QACrDC,OAAO,CAACC,GAAG,CAAC,KAAK,EAAEF,QAAQ,CAACG,MAAM;QAClC,IAAIH,QAAQ,CAACG,MAAK,KAAM,GAAG,EAAE;UAC3B,IAAI,CAACX,SAAQ,GAAIQ,QAAQ,CAACZ,IAAI,CAACgB,OAAO;UACtC,IAAI,CAACX,SAAS,GAAEO,QAAQ,CA