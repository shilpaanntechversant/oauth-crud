        headingValue : \"User Listing Page\",\n        loading:false,\n        errorMessage:null,\n        usersList:[],\n        \n      }\n    },\n    mounted(){\n      this.isLoading = true;\n      this.userListAction().then((response) => {\n        if (response.status.code === 200) {\n          this.usersList = response.data;\n          this.isLoading = false;\n        }\n      });\n    },\n    methods:{\n      ...mapActions({\n        userListAction: 'userList',\n      }),\n  \n      },\n      computed: {\n        ...mapGetters({\n          userList: 'getUserList',\n        })\n      }\n    \n}\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}