{"ast":null,"code":"import { mapActions, mapGetters } from 'vuex';\nimport SpinnerComponent from '@/components/SpinnerComponent.vue';\nimport HeaderComponent from '@/components/HeaderComponent.vue';\nexport default {\n  name: 'UserList',\n  components: {\n    SpinnerComponent,\n    HeaderComponent\n  },\n  data() {\n    return {\n      headingValue: \"User Listing Page\",\n      loading: false,\n      errorMessage: null,\n      usersList: []\n    };\n  },\n  mounted() {\n    this.isLoading = true;\n    this.userListAction().then(response => {\n      if (response.status.code === 200) {\n        this.usersList = response.data;\n        this.isLoading = false;\n      }\n    });\n  },\n  methods: {\n    ...mapActions({\n      userListAction: 'userList'\n    })\n  },\n  computed: {\n    ...mapGetters({\n      userList: 'getUserList'\n    })\n  }\n};","map":{"version":3,"mappings":"AAiDA,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAC7C,OAAOC,gBAAe,MAAQ,mCAAkC;AAChE,OAAOC,eAAc,MAAQ,kCAAiC;AAE9D,eAAe;EACXC,IAAI,EAAC,UAAU;EACfC,UAAU,EAAC;IACTH,gBAAgB;IAChBC;EACF,CAAC;EACDG,IAAI,GAAE;IACJ,OAAM;MACJC,YAAW,EAAI,mBAAmB;MAClCC,OAAO,EAAC,KAAK;MACbC,YAAY,EAAC,IAAI;MACjBC,SAAS,EAAC;IAEZ;EACF,CAAC;EACDC,OAAO,GAAE;IACP,IAAI,CAACC,SAAQ,GAAI,IAAI;IACrB,IAAI,CAACC,cAAc,EAAE,CAACC,IAAI,CAAEC,QAAQ,IAAK;MACvC,IAAIA,QAAQ,CAACC,MAAM,CAACC,IAAG,KAAM,GAAG,EAAE;QAChC,IAAI,CAACP,SAAQ,GAAIK,QAAQ,CAACT,IAAI;QAC9B,IAAI,CAACM,SAAQ,GAAI,KAAK;MACxB;IACF,CAAC,CAAC;EACJ,CAAC;EACDM,OAAO,EAAC;IACN,GAAGlB,UAAU,CAAC;MACZa,cAAc,EAAE;IAClB,CAAC;EAED,CAAC;EACDM,QAAQ,EAAE;IACR,GAAGlB,UAAU,CAAC;MACZmB,QAAQ,EAAE;IACZ,CAAC;EACH;AAEN","names":["mapActions","mapGetters","SpinnerComponent","HeaderComponent","name","components","data","headingValue","loading","errorMessage","usersList","mounted","isLoading","userListAction","then","response","status","code","methods","computed","userList"],"sourceRoot":"","sources":["/home/shilpa/Public/my-work/code/demo-authentication/authenticate-vue/src/views/UserList.vue"],"sourcesContent":["